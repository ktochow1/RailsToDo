<h1>Start a New Project!</h1><br><br><br><br>


<%= form_with model: @pa, url: '/users/:user_id/project_assignments' do |f| %>
<%#binding.pry%>
  <label>Location: </label><%= f.text_field :location %><br><br><br>
  <label>Today's Date: </label><%= select_date Date.today, prefix: :created_at %><br><br><br>
  <label>Lists: </label><%= f.collection_select :list, List.all, :id, :title %><br><br><br>
  <label>User: </label><%= f.collection_select :user, User.all, :id, :username %>
  <%= f.submit "Create" %>
<%end%>
