<h1>Welcome back! Login</h1>

<%= link_to "Login Via Facebook", "/auth/facebook" %>
<%= form_for(:session, url: login_path, method: "POST") do |f| %>
  Email: <%= f.text_field :email %><br>
  Password: <%= f.password_field :password %><br>
  Log In <%= f.submit %>
<% end %><br>
<br>


<%= form_for(User.new) do |f| %>
  Email: <%= f.text_field :email %><br>
  Password: <%= f.password_field :password %><br>
  Sign Up <%= f.submit %>
<% end %>
